    <v:String x:Key="lableNikname">Your Nikname</v:String>
    <v:String x:Key="lablePassword">Your Password</v:String>
    <v:String x:Key="NiknameOrPasswordError">Nikname or password entered incorrectly</v:String>
    <v:String x:Key="ShowPasswordText">Show Password</v:String>

