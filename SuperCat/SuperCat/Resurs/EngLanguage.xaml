    <v:String x:Key="NeedEmailText">E-mail</v:String>
    <v:String x:Key="Day">Day</v:String>
    <v:String x:Key="Month">Month</v:String>
    <v:String x:Key="Year">Year</v:String>
    <v:String x:Key="Man">Man</v:String>
    <v:String x:Key="Woman">Woman</v:String>
    <v:String x:Key="EmailError">Uncorrect E-mail (example SuperCat15@gmail.com)</v:String>

    <!-- Lists -->
    <v:String x:Key="RealName">Real Name:</v:String>
    <v:String x:Key="Nickname">Nickname:</v:String>
    <v:String x:Key="DataBirth">Years:</v:String>
    <v:String x:Key="GenderText">Gender:</v:String>
    <v:String x:Key="e-mail">e-mail:</v:String>
    
